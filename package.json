{
  "name": "oliver-coding-blog",
  "description": "Blog for Daniel Oliver",
  "version": "1.0.0",
  "author": "Daniel Oliver <danieloliversoftware@gmail.com>",
  "dependencies": {
    "bulma": "^0.6.2",
    "font-awesome": "^4.7.0",
    "gatsby": "^1.9.246",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-catch-links": "^1.0.19",
    "gatsby-plugin-feed": "^1.3.11",
    "gatsby-plugin-google-analytics": "^1.0.11",
    "gatsby-plugin-manifest": "^1.0.8",
    "gatsby-plugin-netlify": "^1.0.14",
    "gatsby-plugin-nprogress": "^1.0.2",
    "gatsby-plugin-offline": "^1.0.18",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-sass": "^1.0.12",
    "gatsby-plugin-sharp": "^1.6.41",
    "gatsby-plugin-sitemap": "^1.2.20",
    "gatsby-plugin-twitter": "^1.0.20",
    "gatsby-remark-autolink-headers": "^1.4.16",
    "gatsby-remark-copy-linked-files": "^1.5.31",
    "gatsby-remark-images": "^1.5.60",
    "gatsby-remark-prismjs": "^1.2.24",
    "gatsby-remark-responsive-iframe": "^1.4.18",
    "gatsby-source-filesystem": "^1.5.29",
    "gatsby-source-goodreads": "^0.1.1",
    "gatsby-transformer-remark": "^1.7.39",
    "gatsby-transformer-sharp": "^1.6.22",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "react-share": "^2.1.0",
    "react-twitter-widgets": "^1.7.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "prettier": "^1.11.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.jsx' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
